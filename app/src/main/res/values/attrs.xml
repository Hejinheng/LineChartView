<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="LineChartView">
    <attr name="axesColor" format="color"/> <!--坐标轴颜色-->
    <attr name="axesWidth" format="dimension"/><!--坐标轴宽度-->
    <attr name="textColorX" format="color"/> <!--x轴字体颜色-->
    <attr name="textColorY" format="color"/><!--y轴字体颜色-->
    <attr name="textSizeX" format="dimension"/> <!--x轴字体大小-->
    <attr name="textSizeY" format="dimension"/><!--y轴字体大小-->
    <attr name="lineOneColor" format="color"/> <!--第一条折线的颜色-->
    <attr name="lineTwoColor" format="color"/> <!--第二条折线的颜色-->
    <attr name="bgColor" format="color"/> <!--背景色-->
  </declare-styleable>
</resources>
